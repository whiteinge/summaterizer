{ "env":
    { "browser": true
    , "node": true
    }
, "parser": "babel-eslint"
, "plugins":
    ["react"
    ]
, "ecmaFeatures":
    { "arrowFunctions": true
    , "blockBindings": true
    , "defaultParams": true
    , "destructuring": true
    , "forOf": true
    , "jsx": true
    , "modules": true
    , "objectLiteralShorthandMethods": true
    , "objectLiteralShorthandProperties": true
    , "restParams": true
    , "spread": true
    , "templateStrings": true,
    }
, "rules":
    // Recommended.
    { "comma-dangle": [2, "always-multiline"]
    , "no-cond-assign": 2
    , "no-console": 2
    , "no-constant-condition": 2
    , "no-control-regex": 2
    , "no-debugger": 2
    , "no-dupe-args": 2
    , "no-dupe-keys": 2
    , "no-duplicate-case": 2
    , "no-empty": 2
    , "no-empty-character-class": 2
    , "no-ex-assign": 2
    , "no-extra-boolean-cast": 2
    , "no-extra-semi": 2
    , "no-func-assign": 2
    , "no-inner-declarations": 2
    , "no-invalid-regexp": 2
    , "no-irregular-whitespace": 2
    , "no-negated-in-lhs": 2
    , "no-obj-calls": 2
    , "no-regex-spaces": 2
    , "no-sparse-arrays": 2
    , "no-unexpected-multiline": 2
    , "no-unreachable": 2
    , "use-isnan": 2
    , "valid-typeof": 2
    , "semi": 2

    // Best Practices.
    , "block-scoped-var": 2
    , "consistent-return": 2
    , "dot-location": [2, "property"]
    , "eqeqeq": 2
    , "no-alert": 2
    , "no-caller": 2
    , "no-div-regex": 2
    , "no-eq-null": 2
    , "no-eval": 2
    , "no-fallthrough": 2
    , "no-floating-decimal": 2
    , "no-implicit-coercion": 2
    , "no-implied-eval": 2
    , "no-iterator": 2
    , "no-labels": 2
    , "no-lone-blocks": 2
    , "no-loop-func": 2
    , "no-multi-spaces": 2
    , "no-multi-str": 2
    , "no-native-reassign": 2
    , "no-new-func": 2
    , "no-new-wrappers": 2
    , "no-new": 2
    , "no-octal-escape": 2
    , "no-octal": 2
    , "no-param-reassign": 2
    , "no-process-env": 2
    , "no-proto": 2
    , "no-redeclare": 2
    , "no-return-assign": 2
    , "no-script-url": 2
    , "no-self-compare": 2
    , "no-sequences": 2
    , "no-throw-literal": 2
    , "no-unused-expressions": 2
    , "no-useless-call": 2
    , "no-void": 2
    , "no-warning-comments": 1
    , "no-with": 2
    , "radix": 2
    , "wrap-iife": 2
    , "yoda": 2

    // Strict mode.
    , "strict": [2, "global"]

    // Variables.
    , "no-catch-shadow": 2
    , "no-delete-var": 2
    , "no-label-var": 2
    , "no-shadow-restricted-names": 2
    , "no-shadow": 2
    , "no-undef-init": 2
    , "no-undef": 2
    , "no-undefined": 2
    , "no-unused-vars": 2
    , "no-use-before-define": [2, "nofunc"]

    // Style.
    , "array-bracket-spacing": 2
    , "brace-style": [2, "1tbs", {"allowSingleLine": true}]
    , "camelcase": 0
    , "comma-spacing": 2
    , "comma-style": 2
    , "computed-property-spacing": 2
    , "consistent-this": [2, "that"]
    , "eol-last": 2
    , "indent": 2
    , "key-spacing": 2
    , "linebreak-style": 2
    , "no-mixed-spaces-and-tabs": 2
    , "no-multiple-empty-lines": [2, {"max": 2}]
    , "no-nested-ternary": 2
    , "no-spaced-func": 2
    , "no-trailing-spaces": 2
    , "object-curly-spacing": 2
    , "operator-assignment": [2, "always"]
    , "operator-linebreak": 2
    , "quotes": [2, "single", "avoid-escape"]
    , "semi-spacing": 2
    , "space-after-keywords": 2
    , "space-before-blocks": 2
    , "space-before-function-paren": [2, "never"]
    , "space-in-parens": 2
    , "space-infix-ops": 2
    , "space-return-throw-case": 2
    , "space-unary-ops": 2
    , "spaced-comment": 2
    , "max-len": [2, 80, 4]

    // ES6
    , "arrow-spacing": 2
    , "no-const-assign": 1

    // React/JSX
    , "react/display-name": 2
    , "react/no-did-mount-set-state": 2
    , "react/no-did-update-set-state": 2
    , "react/prop-types": 2
    , "react/sort-comp": 2
    }
}
